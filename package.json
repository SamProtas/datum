{
  "name": "@nll/datum",
  "version": "2.6.0",
  "description": "ADT for handling fetchable and refreshable data",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "es6"
  ],
  "sideEffects": false,
  "scripts": {
    "build-es6": "tsc",
    "build-lib": "tsc --build tsconfig.lib.json",
    "build": "npm run build-es6 && npm run build-lib",
    "docs": "npx docs-ts",
    "jest-clear-cache": "jest --clearCache",
    "jest": "jest --ci",
    "coveralls": "cat ./coverage/lcov.info | npx coveralls",
    "prepublishOnly": "npm run test && npm run build",
    "release": "npm run test && npm run build && npm run docs && npx standard-version",
    "test": "npm run jest-clear-cache && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nullpub/datum.git"
  },
  "keywords": [
    "fp-ts",
    "adt",
    "AsyncData"
  ],
  "author": "Brandon Blaylock <brandon@null.pub> (https://blaylock/dev)",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/nullpub/datum/issues"
  },
  "homepage": "https://github.com/nullpub/datum#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "coveralls": "^3.0.6",
    "docs-ts": "^0.2.1",
    "fp-ts": "^2.0.5",
    "jest": "^24.9.0",
    "standard-version": "^5.0.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.3.3333"
  },
  "peerDependencies": {
    "fp-ts": "^2.0.5"
  }
}
